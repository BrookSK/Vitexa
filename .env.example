# Configurações do Banco de Dados
DB_HOST=localhost
DB_NAME=vitexa_db
DB_USER=root
DB_PASS=

# Configurações da Aplicação
APP_URL=http://localhost
APP_ENV=development
APP_DEBUG=true
TIMEZONE=America/Sao_Paulo

# Configurações da API OpenAI
OPENAI_API_KEY=sua_chave_openai_aqui
OPENAI_API_BASE=https://api.openai.com/v1

# Configurações de Cache
CACHE_ENABLED=true
CACHE_TTL=3600
CACHE_DIR=/tmp/vitexa_cache/

# Configurações de Email (opcional)
MAIL_ENABLED=true
MAIL_FROM=noreply@vitexa.com
MAIL_HOST=localhost
MAIL_PORT=587
MAIL_USERNAME=
MAIL_PASSWORD=

# Configurações de Push Notifications (opcional)
PUSH_ENABLED=false
FIREBASE_SERVER_KEY=

# Configurações de Webhook (opcional)
WEBHOOK_URL=

# Configurações de Cron Jobs
CRON_TOKEN=vitexa_cron_2024

# Configurações de Upload
UPLOAD_MAX_SIZE=5242880
UPLOAD_ALLOWED_TYPES=jpg,jpeg,png,gif

# Configurações de Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600

# Configurações de Log
LOG_ENABLED=true
LOG_LEVEL=info
LOG_DIR=/var/log/vitexa/


